package Excercises;

public class SimpleCal {

	public static void main(String[] args) {
		int i,n,s;
		n=2;
		System.out.println(n);
		for(i=1;i<=10;i++)
		{
			s=n*i;
			System.out.print(n);
			System.out.print("*");
			System.out.print(i);
			System.out.print("=");
			System.out.print(s);
			System.out.println();
		}
		

	}

}
