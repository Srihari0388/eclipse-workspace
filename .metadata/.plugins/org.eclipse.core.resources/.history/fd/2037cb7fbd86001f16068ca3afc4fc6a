
public class Multiplication {

	public static void main(String[] args) {
		int i,j,k;
		for (i=1;i<=20;i++)
		{
			System.out.println("-------------");
			System.out.println("Table of : "+i);
			System.out.println("-------------");
			for(j=1;j<=10;j++)
			{
				k=i*j;
				System.out.println(i+"*"+j+"="+k);
				
			}
		}

	}

}
